{
    "name": "ecommerce",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^5.2.5",
        "@commercetools/importapi-sdk": "^4.6.2",
        "@commercetools/platform-sdk": "^5.0.0",
        "@commercetools/sdk-client-v2": "^2.2.0",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.39",
        "antd": "^5.8.1",
        "config.json": "^0.0.4",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "lint-staged": "^13.2.3",
        "mobx": "^6.10.0",
        "mobx-react-lite": "^4.0.3",
        "node-fetch": "^2.6.1",
        "rc-field-form": "^1.37.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.14.2",
        "react-scripts": "^5.0.1",
        "react-toastify": "^9.1.3",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "cross-env NODE_PATH=./src react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint . --ext .ts",
        "jest": "jest --verbose --runInBand --forceExit",
        "format": "npx prettier --write .",
        "prepare": "cd .. && husky install ecommerce/.husky"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run format",
            "pre-push": "npm run lint"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@jest/globals": "^29.5.0",
        "@types/node-fetch": "^2.6.4",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "css-loader": "^6.8.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-myconfig": "^0.2.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "filemanager-webpack-plugin": "^8.0.0",
        "html-webpack-plugin": "^5.5.1",
        "husky": "^8.0.3",
        "prettier": "^3.0.1",
        "pretty-quick": "^3.1.3",
        "sass": "^1.64.2",
        "style-loader": "^3.3.3",
        "ts-jest": "^29.1.1"
    },
    "compilerOptions": {
        "moduleResolution": "node"
    }
}
